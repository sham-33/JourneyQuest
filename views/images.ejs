<div class="container-fluid">
  <div class="row">
    <!-- List div on the left -->
    <div class="col-md-4" style="margin-top: 75px">
      <div
        class="d-flex flex-column align-items-stretch flex-shrink-0 bg-body-tertiary rating-div"
        style="width: 380px">
        <a
          href="/"
          class="d-flex align-items-center flex-shrink-0 p-3 link-body-emphasis text-decoration-none border-bottom">
          <svg class="bi pe-none me-2" width="30" height="24">
            <use xlink:href="#bootstrap"></use>
          </svg>
          <span class="fs-5 fw-semibold">Most Rated Countries</span>
        </a>
        <div
          class="list-group list-group-flush border-bottom scrollarea"
          style="max-height: 300px; overflow-y: auto">
          <!-- Your list items here -->
          <% ratings.forEach(function(rating) { %>
          <a
            href="#"
            class="list-group-item list-group-item-action py-3 lh-sm"
            aria-current="true">
            <div
              class="d-flex w-100 align-items-center justify-content-between">
              <strong class="mb-1"><%= rating.country_name %></strong>
              <small><%= rating.rating %></small>
            </div>
          </a>
          <% }); %>
          <!-- More list items -->
        </div>
      </div>
    </div>

    <!-- Form div on the right -->
    <div class="col-md-8">
      <div
        class="container px-4 py-5"
        style="display: inline-block; margin-right: 20px">
        <div class="row align-items-center g-lg-5 py-5">
          <div class="col-md-10 mx-auto col-lg-5">
            <form
              class="p-4 p-md-5 border rounded-3 bg-body-tertiary"
              action="/getimages"
              method="post">
              <div class="form-floating mb-3">
                <input
                  type="text"
                  class="form-control"
                  id="floatingInput"
                  placeholder=""
                  fdprocessedid="b6oo0g"
                  name="user" />
                <label for="floatingInput">Username</label>
              </div>
              <div class="form-floating mb-3">
                <input
                  type="text"
                  class="form-control"
                  id="floatingPassword"
                  placeholder="Password"
                  fdprocessedid="74v2mu"
                  name="country" />
                <label for="floatingPassword">Visited country</label>
              </div>
              <button
                class="w-100 btn btn-lg btn-primary"
                type="submit"
                fdprocessedid="zdxyok">
                Images
              </button>
              <hr class="my-4" />
            </form>
          </div>
        </div>
      </div>
    </div>
  </div>

  <!-- Carousel section -->
  <div id="Views" class="container" style="margin-bottom: 30px">
    <div id="carouselExample" class="carousel slide">
      <div class="carousel-inner">
        <% users.forEach(function(user) { %> <% const filePath = user.image; %>
        <% const fileName = filePath.replace('uploads\\', ''); %>
        <div class="carousel-item active" style="height: 700px">
          <img src="<%= fileName %>" class="d-block w-100" alt="..." />
        </div>
        <% }); %>
      </div>
      <button
        class="carousel-control-prev"
        type="button"
        data-bs-target="#carouselExample"
        data-bs-slide="prev">
        <span class="carousel-control-prev-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Previous</span>
      </button>
      <button
        class="carousel-control-next"
        type="button"
        data-bs-target="#carouselExample"
        data-bs-slide="next">
        <span class="carousel-control-next-icon" aria-hidden="true"></span>
        <span class="visually-hidden">Next</span>
      </button>
    </div>
  </div>

  <%- include("partials/footer.ejs") %>
</div>
